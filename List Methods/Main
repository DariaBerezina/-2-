using ConsoleApp1;
class Program
{
    public static void Values(List<int> list)
    {
        System.Collections.IEnumerator enumerator = list.GetEnumerator();
        Console.WriteLine("Ваш лист:");
        while (enumerator.MoveNext())
        {
            Console.Write("\t{0}", enumerator.Current);
        }
        Console.WriteLine();
    }
    static void Main()
    {
        Menu m = new Menu();

        List<int> list = new List<int>();
        Console.WriteLine("Введите элемент списка. Если желаете прекратить заполнение списка введите \"end\".");
        string str = Menu.check_p1();
        while (str != "end" || str != "END" || str != "End" || str != "\"end\""
            || str != "\"END\"" || str != "\"End\"")
        {
            list.Add(int.Parse(str));
            str = Menu.check_p1();
        }

        Console.WriteLine("Ваш лист был успешно заполнен! Нажмите любую клавишу для продолжения.");
        Console.ReadKey();

        Console.Clear();
        Values(list);
        Console.ReadKey();
        Console.Clear();

        m.my_list = list;
        m.MainMenu();
    }
}
