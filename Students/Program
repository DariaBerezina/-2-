using System;
using qwerty;
class Program
{
    static void Main()
    {
        Console.WriteLine("Введите количество студентов: ");
        int n = Convert.ToInt32(Console.ReadLine());
        Console.Clear();
        Student[] arr = new Student[n];
        for (int i = 0; i < n; i++)
        {
            arr[i] = Student.Print();
        }
        Menu.Print(arr);
    }
}
    
